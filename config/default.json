{
  "app": {
    "https": false,
    "port": 2000
  },
  "aws": {
    "credentials" : {
      "key": "accessKeyId",
      "secret": "secretAccessKey"
    },
    "region": "aws region"
  },
  "sqs": {
    "apiVersion": "2012-11-05",
    "queueUrl": "sqs url",
    "region": "sqs region",
    "deadletter": {
      "queueUrl": "dead-letter-queue-url",
      "region": "sqs region"
    }
  },
  "lambda": {
    "apiVersion": "2015-03-31"
  },
  "lrs": {
    "ingestService": "lrs-ingest-microservice",
    "region": "lambda region"
  },
  "logger": {
    "streams": [
      {
        "level": "info",
        "stream": "stdout"
      },
      {
        "level": "debug",
        "path": "logs/app.log"
      }
    ],
    "useColor": false
  },
  "poller": {
    "concurrencyCount": 1
  }
}
